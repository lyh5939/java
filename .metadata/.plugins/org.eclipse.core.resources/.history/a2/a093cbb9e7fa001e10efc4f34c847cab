import java.util.Scanner;

public class Ex11_Q3
{

	public static void main(String[] args)
	{
		
		Scanner sc = new Scanner(System.in);	
		
		System.out.println("정수를 입력하시요 : ");
		int num = sc.nextInt();
		int result = 1;
		
		while(num  >  1)
		{
			result = result * num;
			num--;
			System.out.print( num + "*");
		}
			
		System.out.println(" = " + result);
		
	}

}
