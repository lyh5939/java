import java.util.Scanner;

public class Ex11_Q3
{

	public static void main(String[] args)
	{
		
		Scanner sc = new Scanner(System.in);	
		
		System.out.println("정수를 입력하시요 : ");
		int num = sc.nextInt();
		long result = 1;
		
		while( num  >=  1)
		{
			result = result * num;
			
			if(num == 1)
			{
				System.out.print( num + " = " );
			}
			else
			{
				System.out.print( num + " * ");
			}
			
			num--;
		}
			
		System.out.println( result);
		
	}

}
